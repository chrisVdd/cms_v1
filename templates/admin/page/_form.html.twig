{{ form_start(form) }}

    <div class="row">
        <div class="col">
            {{ form_row(form.title) }}
        </div>
        <div class="row">
            {{ form_row(form.subTitle) }}
        </div>
    </div>

    {{ form_row(form.content) }}
    
    <div class="custom-control custom-switch">
        {{ form_row(form.online) }}

    <button class="btn btn-success btn-user btn-block">
        {{ button_label|default('Save') }}
    </button>
{{ form_end(form) }}